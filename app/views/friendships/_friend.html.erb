  <%= turbo_frame_tag friend do %>
    <div class="col-md-4">
      <div class="card mb-4">
        <div class="card-body">
          <h5 class="card-title d-flex justify-content-between">
            <%= friend.username %>
            <%= button_to '', friendship_path(friend.id), method: :delete, data: { turbo_confirm: '友達やめますか？' }, class: 'btn-close'%>
          </h5>
          <p class="card-text"><%= friend.email %></p>
        </div>
      </div>
    </div>
  <% end %>
