<h1>Login</h1>
<%= form_with url: login_path, class: 'form-group' do |form| %>
  <div class="mb-3">
    <%= form.label :email, class: 'form-label' %>
    <%= form.email_field :email, class: 'form-control' %>
  </div>
  <div class="mb-3">
    <%= form.label :password, class: 'form-label' %>
    <%= form.password_field :password, class: 'form-control', autocomplete: 'password' %>
  </div>
  <%= form.submit "Login", class: 'btn btn-primary' %>
<% end %>
